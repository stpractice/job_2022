{
    "name": "eu-141/job_2022",
    "type": "project",
    "description": "Job site",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo_mysql": "*",
        "mikecao/flight": "^2.0",
        "illuminate/validation": "^9.7",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "authors": [
        {
            "name": "EU-141",
            "email": "eu-141@example.com"
        }
    ],
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "php -c php.ini -S localhost:8000 -r public"
        ],
        "fix": "vendor/bin/php-cs-fixer fix"
    },
    "scripts-descriptions": {
        "dev": "Run developer server.",
        "fix": "Fix code for PSR-12 standart."
    }
